{
  "name": "occp",
  "version": "1.0.0",
  "description": "Ocpp Interface",
  "main": "server.js",
  "scripts": {
    "development": "nodemon",
    "build": "webpack --mode=production --config  webpack.config.server.js",
    "start": "NODE_ENV=production node ./dist/server.generated.js",
    "format": "prettier --write  'server/**/*.js'"
  },
  "author": "Jorge Betancur",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.27.1",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.5.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "debug": "3.1.0",
    "enjoi": "3.2.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.2.0",
    "joi": "13.1.2",
    "joi-date-extensions": "1.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.15",
    "nanoid": "^3.1.30",
    "uuid": "^8.3.2",
    "webpack": "^5.64.1",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2",
    "ws": "4.0.0"
  },
  "engines": {
    "node": "16.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,json}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "node_modules",
      "dist",
      "package-lock.json"
    ]
  }
}
